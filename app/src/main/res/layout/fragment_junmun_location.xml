<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".MapActivity"
    android:background="#595959"
    android:clickable="true">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        >

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/constraintLayout"
            android:layout_width="match_parent"
            android:layout_height="20dp"
            android:background="#2F5597"
            android:orientation="horizontal"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">
            <ImageButton
                android:id="@+id/btn_back_super"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_gravity="center"
                android:layout_marginStart="10dp"
                android:background="?android:attr/selectableItemBackgroundBorderless"
                android:onClick="onButtonClick"
                android:src="@drawable/back_white"

                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/textView"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_marginStart="10dp"
                android:gravity="center"
                android:text="위치 보고"
                android:textColor="#fff"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/linearLayout2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="2dp"
            android:background="@drawable/border"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/constraintLayout"
            app:layout_constraintBottom_toTopOf="@+id/junmun_btn_write_main"
            >


            <TextView
                android:id="@+id/junmun_location_tv_priority"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="우선 순위 : "
                android:textColor="@color/black"
                android:layout_marginStart="10dp"
                android:clickable="true"
                app:layout_constraintBottom_toBottomOf="@id/junmun_location_sp_priority"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <Spinner
                android:id="@+id/junmun_location_sp_priority"
                android:layout_width="wrap_content"
                android:layout_height="20dp"
                android:dropDownWidth="wrap_content"
                android:layout_marginStart="10dp"
                android:entries="@array/unit"
                android:theme="@style/SpinnerTheme"
                android:layout_marginBottom="5dp"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintStart_toEndOf="@id/junmun_location_tv_priority"
                app:layout_constraintBottom_toTopOf="@id/divider1"/>

            <View
                android:id="@+id/divider1"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_centerInParent="true"
                android:layout_marginTop="2dp"
                android:background="#000"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/junmun_location_tv_priority"
                app:layout_constraintBottom_toTopOf="@id/junmun_location_et_time"/>


            <TextView
                android:id="@+id/junmun_location_tv_time"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="10dp"
                android:gravity="center_horizontal"
                android:text="위치 : "
                android:maxLength="25"
                android:imeOptions="flagNoExtractUi"
                android:textColor="@color/black"
                app:layout_constraintBottom_toBottomOf="@id/junmun_location_et_time"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@id/junmun_location_et_time" />

            <EditText
                android:id="@+id/junmun_location_et_time"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginEnd="10dp"
                android:hint="위치 입력"
                android:textSize="12sp"
                android:maxLength="25"
                android:imeOptions="actionDone|flagNoExtractUi"
                app:layout_constraintStart_toEndOf="@id/junmun_location_tv_time"
                app:layout_constraintTop_toBottomOf="@id/divider1"
                app:layout_constraintBottom_toTopOf="@id/divider2"
                app:layout_constraintEnd_toEndOf="parent"
                />

            <View
                android:id="@+id/divider2"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_centerInParent="true"
                android:background="#000"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/junmun_location_et_time"
                app:layout_constraintBottom_toTopOf="@id/junmun_location_et_location_observe"/>

            <TextView
                android:id="@+id/junmun_location_tv_location_observe"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="10dp"
                android:gravity="center_horizontal"
                android:text="방향 : "
                android:imeOptions="flagNoExtractUi"
                android:textColor="@color/black"
                app:layout_constraintBottom_toBottomOf="@id/junmun_location_et_location_observe"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@id/junmun_location_et_location_observe" />

            <EditText
                android:id="@+id/junmun_location_et_location_observe"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginEnd="10dp"
                android:hint="방향 입력"
                android:imeOptions="actionDone|flagNoExtractUi"
                android:maxLength="25"
                android:textSize="12sp"
                app:layout_constraintWidth_percent="0.5"
                app:layout_constraintBottom_toTopOf="@id/divider3"
                app:layout_constraintStart_toEndOf="@id/junmun_location_tv_location_observe"
                app:layout_constraintTop_toBottomOf="@id/divider2" />

            <View
                android:id="@+id/divider3"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_centerInParent="true"
                android:background="#000"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/junmun_location_et_location_observe"
                app:layout_constraintBottom_toTopOf="@id/junmun_location_et_location_object"/>

            <TextView
                android:id="@+id/junmun_location_tv_location_object"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="10dp"
                android:gravity="center_horizontal"
                android:text="속도 : "
                android:textColor="@color/black"

                app:layout_constraintBottom_toBottomOf="@id/junmun_location_et_location_object"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@id/junmun_location_et_location_object"/>

            <EditText
                android:id="@+id/junmun_location_et_location_object"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginEnd="10dp"
                android:hint="속도 입력"
                android:textSize="12sp"
                android:maxLength="25"
                android:imeOptions="actionDone|flagNoExtractUi"
                app:layout_constraintWidth_percent="0.5"
                app:layout_constraintStart_toEndOf="@id/junmun_location_tv_location_object"
                app:layout_constraintTop_toBottomOf="@id/divider3"
                app:layout_constraintBottom_toTopOf="@id/divider4"
                />

            <View
                android:id="@+id/divider4"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_centerInParent="true"
                android:background="#000"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/junmun_location_et_location_object"/>

            <TextView
                android:id="@+id/junmun_location_tv_height"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="10dp"
                android:gravity="center_horizontal"
                android:text="고도 : "
                android:textColor="@color/black"
                app:layout_constraintBottom_toBottomOf="@id/junmun_location_et_height"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@id/junmun_location_et_height"/>

            <EditText
                android:id="@+id/junmun_location_et_height"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginEnd="10dp"
                android:hint="고도 입력"
                android:textSize="12sp"
                android:maxLength="25"
                android:imeOptions="actionDone|flagNoExtractUi"
                app:layout_constraintWidth_percent="0.5"
                app:layout_constraintStart_toEndOf="@id/junmun_location_tv_height"
                app:layout_constraintTop_toBottomOf="@id/divider4"

                />

        </androidx.constraintlayout.widget.ConstraintLayout>

        <Button
            android:id="@+id/junmun_btn_write_main"
            android:layout_width="0dp"
            android:layout_height="35dp"
            android:background="@drawable/bg_custom_button4"
            android:text="전송"
            android:textColor="@color/white"
            android:textSize="20sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/btn_back_under"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/linearLayout2" />

        <Button
            android:id="@+id/btn_back_under"
            android:onClick="onButtonClick"
            android:layout_width="0dp"
            android:layout_height="35dp"
            android:text="취소"
            android:textSize="20sp"
            android:textColor="@color/white"
            android:background="@drawable/bg_custom_button4"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/junmun_btn_write_main"
            app:layout_constraintTop_toTopOf="@id/junmun_btn_write_main" />




    </androidx.constraintlayout.widget.ConstraintLayout>

</FrameLayout>